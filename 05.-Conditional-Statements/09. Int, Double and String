using System;
namespace Int__Double__String
{
    class Program
    {
        static void Main()
        {
            string type = Console.ReadLine();
            switch(type)
            {
                case "integer": int numberInt = int.Parse(Console.ReadLine());
                    Console.WriteLine(numberInt + 1); break;
                case "real": double numberDouble = double.Parse(Console.ReadLine());
                    Console.WriteLine("{0:F2}", numberDouble + 1); break;
                case "text": string text = Console.ReadLine();
                    Console.WriteLine(text + "*"); break;
            }
        }
    }
}
