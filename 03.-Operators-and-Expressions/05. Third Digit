using System;
namespace _3_digit
{
    class Program
    {
        static void Main()
        {
            uint N = uint.Parse(Console.ReadLine());
            uint a = (N / 100);
            uint x = (a % 10);



            if (x == 7)
            {
                Console.WriteLine("true");
            }
            else
            {
                Console.WriteLine("false {0}", x);
            }
        }
    }
}
