using System;
namespace N_th_Bit
{
    class Program
    {
        static void Main()
        {
            ulong p = ulong.Parse(Console.ReadLine());
            int n = int.Parse(Console.ReadLine());

            ulong mask = p >> n;
            ulong bit = mask & 1;  
             
            Console.WriteLine(bit);
        }
    }
}
