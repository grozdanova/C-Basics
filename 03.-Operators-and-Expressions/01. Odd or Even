using System;
namespace odd_or_even
{
    class Program
    {
        static void Main()
        {
            int number = int.Parse(Console.ReadLine());
            string result = "odd";
            if (number % 2 == 0)
            {
             result= "even";

            }
            Console.WriteLine("{0} {1}", result, number);
        }
    }
}
