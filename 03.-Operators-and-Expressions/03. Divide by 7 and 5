using System;
namespace Divide_by_7_and_5
{
    class Program
    {
        static void Main()
        {
            int number = int.Parse(Console.ReadLine());
            string result = "false";
            if (number % 7 == 0 && number % 5 == 0)
            {
                result = "true";
            }
            Console.WriteLine("{0} {1}", result, number);
        }
    }
}
